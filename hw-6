from itertools import count, cycle

n = int(input('Ввод стартового числа: '))
for el in count(n):
    if el > 10:
        break
    else:
        print(el)
print(' ---- 2 пункт задания -----')
l = ['a', 'b', 'c', 'd', 'e', 'f']
c = 0
for el in cycle(l):
    if c > (len(l) - 1):
        break
    print(el)
    c += 1
